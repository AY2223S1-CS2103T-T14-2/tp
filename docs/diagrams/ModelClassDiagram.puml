@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model <<Rectangle>>{
Class "<<interface>>\nReadOnlyAddressBook" as ReadOnlyAddressBook
Class "<<interface>>\nReadOnlyTaskList" as ReadOnlyTaskList
Class "<<interface>>\nReadOnlyUserPrefs" as ReadOnlyUserPrefs
Class "<<interface>>\nModel" as Model
Class AddressBook
Class TaskList
Class ModelManager
Class UserPrefs

Class UniquePersonList
Class Person
Class Address
Class Email
Class Name
Class Phone
Class Tag

Class UniqueTaskList
Class Task
Class TaskName
Class Module
Class Deadline
Class Status

}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

AddressBook .up.|> ReadOnlyAddressBook

TaskList .up.|> ReadOnlyTaskList

UserPrefs .up.|> ReadOnlyUserPrefs

ModelManager .up.|> Model
Model .left.> ReadOnlyAddressBook
Model ..> ReadOnlyTaskList
Model .right.> ReadOnlyUserPrefs
ModelManager -left-> "1" AddressBook
ModelManager --> "1" TaskList
ModelManager -right-> "1" UserPrefs

AddressBook *--> "1" UniquePersonList
UniquePersonList --> "~* all" Person
Person *--> Name
Person *--> Phone
Person *--> Email
Person *--> Address
Person *--> "*" Tag

Name -[hidden]right-> Phone
Phone -[hidden]right-> Address
Address -[hidden]right-> Email

ModelManager -->"~* filtered" Person

TaskList *--> "1" UniqueTaskList
UniqueTaskList --> "~* all" Task
Task *--> TaskName
Task *--> Module
Task *--> Deadline
Task *--> Status

Name -[hidden]right-> Module
Module -[hidden]right-> Deadline
Deadline -[hidden]right-> Status

ModelManager -->"~* filtered" Task

@enduml
